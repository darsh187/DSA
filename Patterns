#include <iostream>
#include <bits/stdc++.h>
using namespace std;


void func1(int n) {
    // Write C++ code here

    for(int i=1; i<=n; i++){
        for(int j=1; j<=n; j++){
            cout<<"*";
        }
        cout<<endl;
    }
}
void func2(int n) {
    // Write C++ code here

    for(int i=1; i<=n; i++){
        for(int j=1; j<=i; j++){
            cout<<"*";
        }
        cout<<endl;
    }
}
int main()
{
    int n;
    cin>>n;
    func1(n);
    func2(n);
    
}
