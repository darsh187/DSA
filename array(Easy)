#include <iostream>

using namespace std;

void largestele(int n, int arr[])
{
    int largest = arr[0];
    for(int i = 0; i < n; i++)
    {
        if(arr[i] > largest)
        {
            largest = arr[i];
        }
    }
    cout << largest;
}
int main()
{
    int n;
    cin >> n;
    int arr[n];
    for(int i = 0; i < n; i++) cin >> arr[i];
    largestele(n, arr);
    return 0;
}
