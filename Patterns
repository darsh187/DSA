        }
        cout<<endl;
    }
}
void func5(int n) {
    // Write C++ code here

    for(int i=1; i<=n; i++){
        for(int j=1; j<=n-i+1; j++){
            cout<<"*";
        }
        cout<<endl;
    }
}
void func6(int n) {
    // Write C++ code here

    for(int i=1; i<=n; i++){
        for(int j=1; j<=n-i+1; j++){
            cout<<j;
        }
        cout<<endl;
    }
}
void func7(int n) {
    // Write C++ code here

    for(int i=0; i<n; i++){
        // space
        for(int j=0; j<n-i-1; j++){
            cout<<" ";
        }
        // star
        for(int j=0; j<2*i+1; j++){
            cout<<"*";
        }
        // space
        for(int j=0; j<n-i-1; j++){
            cout<<" ";
        }
        cout<<endl;
    }
}
void func8(int n) {
    // Write C++ code here

    for(int i=0; i<n; i++){
        // space
        for(int j=0; j<i; j++){
            cout<<" ";
        }
        // star
        for(int j=0; j<2*n-(2*i+1); j++){
            cout<<"*";
        }
        // space
        for(int j=0; j<i; j++){
            cout<<" ";
        }
        cout<<endl;
    }
}
void func9(int n){
    for(int i=1; i<=2*n-1; i++){
        int stars = i;
        if(i > n) stars = 2*n - i;
        for(int j=1; j<=stars; j++){
            cout<<"*";
        }
        cout<<endl;
    }
}
void func10(int n){
    int start = 1;
    for(int i=0; i<=n; i++)
    {
        if (i % 2 == 0) start = 1;
        else start = 0;
        for(int j = 0; j <= i; j++)
        {
            cout<<start;
            start = 1 - start;
        }
        cout<<endl;
    }
}
void func11(int n){
    int space = 2*(n-1);
    for(int i = 1; i<=n; i++)
    {
        for(int j = 1; j <= i; j++)
        {
            cout<<j;
        }
        for(int j = 1; j<=space; j++)
        {
            cout<<" ";
        }
        for(int j = i; j>=1; j--)
        {
            cout<<j;
        }
        cout<<endl;
        space -=2;
    }
}
void func12(int n)
{
    int num = 1;
    for(int i = 1; i <= n; i++)
    {
        for(int j = 1; j <=i; j++)
        {
            cout<<num<<" ";
            num++;
        }
        cout<<endl;
    }
}
void func13(int n)
{
    for(int i = 0; i < n; i++)
    {
        for(char ch = 'A'; ch <= 'A'+i; ch++)
        {
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void func14(int n)
{
    for(int i = 0; i < n; i++)
    {
        for(char ch = 'A'; ch <= 'A'+(n-i-1); ch++)
        {
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void func15(int n)
{
    for(int i = 0; i < n; i++)
    {
        char ch = 'A'+i;
        for(int j = 0; j <= i; j++)
        {
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void func16(int n) {
    // Write C++ code here

    for(int i=0; i<n; i++){
        // space
        for(int j=0; j<n-i-1; j++){
            cout<<" ";
        }
        // charecter
        char ch = 'A';
        int breakpoint = (2*i + 1)/2;
        for(int j = 1; j<=2*i + 1; j++){
            cout<<ch;
            if(j <= breakpoint) ch++;
            else ch--;
        }
        // space
        for(int j=0; j<n-i-1; j++){
            cout<<" ";
        }
        cout<<endl;
    }
}
void func17(int n)
{
    for(int i = 0; i < n; i++)
    {
        for(char ch = 'E'-i;  ch <= 'E'; ch++)
        {
            cout<<ch<<" ";
        }
        cout<<endl;
    }
}
void func18(int n)
{
    int space = 0;
    for(int i = 0; i <= n; i++)
    {
        //star
        for(int j = 1; j <= n-i; j++)
        {
            cout<<"*";
        }
        // space
        for(int j = 1; j <= space; j++)
        {
            cout<<" ";
        }
        //star
        for(int j = 1; j <= n-i; j++)
        {
            cout<<"*";
        }
        space += 2;
        cout<<endl;
    }
    space = 8;
    for(int i = 1; i <= n; i++)
    {
        // star
        for(int j = 1; j <= i; j++)
        {
            cout<<"*";
        }
        // space
        for(int j = 1; j <= space; j++)
        {
            cout<<" ";
        }
        //star
        for(int j = 1; j <= i; j++)
        {
            cout<<"*";
        }
        space -= 2;
        cout<<endl;
    }
}
void func19(int n)
{
    int space = 2*n - 2;
    for(int i = 1; i <= 2*n-1; i++)
    {
        int stars = i;
        if(i > n) stars = 2*n - i;
        // star
        for(int j = 1; j <= stars; j++)
        {
            cout<<"*";
        }
        // space
        for(int j = 1; j <= space; j++)
        {
            cout<<" ";
        }
        //star
        for(int j = 1; j <= stars; j++)
        {
            cout<<"*";
        }
        cout<<endl;
        if (i < n) space -= 2;
        else space += 2;
    }
}
void func20(int n)
{
    for(int i = 0; i < n; i++)
    {
        for(int j = 0; j < n; j++)
        {
            if(i == 0 || j==0 || i == n-1 || j == n-1)
            {
                cout<<"*";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<endl;
    }
}
